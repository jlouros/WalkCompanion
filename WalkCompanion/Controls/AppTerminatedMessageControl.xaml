<UserControl x:Class="WalkCompanion.Controls.AppTerminatedMessageControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:WalkCompanion.Assets.Xaml"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
        
        
        <TextBlock TextWrapping="Wrap" Text="{Binding LocalizedResources.UnexpectedAppTermination, Mode=OneWay, Source={StaticResource LocalizedStrings}}" Canvas.ZIndex="2"/>
        <local:Warning />
    </Grid>
</UserControl>